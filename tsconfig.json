{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "types": ["jest", "node"],
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowImportingTsExtensions": false,
    "module": "esnext"
  },
  "include": [
    "./src/**/*",
    "index.d.ts",
    ".eslintrc.js",
    "jest.setup.js",
    "jest.config.js",
    "babel.config.js",
    "index.js"
  ],
  "exclude": ["**/node_modules", "**/Pods"]
}
